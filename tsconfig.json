{
  "compilerOptions": {
    "strict": true,
    "types": ["react-native"],
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["App/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "tsconfig.effective.json"
  ],
  "extends": "expo/tsconfig.base"
}
