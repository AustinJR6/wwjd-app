    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^3.22.0",
    "stripe": "^18.3.0",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "tsc-alias": "^1.8.8",
    "concurrently": "^8"
  },
  "private": true
