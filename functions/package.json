  "description": "Cloud Functions for Firebase",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc && tsc-alias",
    "build:watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "predeploy": "npm run build"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^4.0.0",
